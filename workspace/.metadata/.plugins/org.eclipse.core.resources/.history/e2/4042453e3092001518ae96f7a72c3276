import java.io.*;

public class Main {

	public static void main(String[] args) throws FileNotFoundException, IOException {
		String[] Files = {"CodeIndex1.bin", "CodeIndex2.bin", "CodeIndex3.bin"};
		UIoutput output = new UIoutput();
		UI Trans1 new UI(output);
		CodeIndex index1 = new CodeIndex("CodeIndex1.bin");
		index1.setupApp();
		index1.readFile();
		index1.finishUp();
		System.out.println();
		CodeIndex index2 = new CodeIndex("CodeIndex2.bin");
		index2.setupApp();
		index2.readFile();
		index2.finishUp();
		System.out.println();
		CodeIndex index3 = new CodeIndex("CodeIndex3.bin");
		index3.setupApp();
		index3.readFile();
		index3.finishUp();
	}
}
		
