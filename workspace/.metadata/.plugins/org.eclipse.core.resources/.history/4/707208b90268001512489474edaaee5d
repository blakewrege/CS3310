import scala.io.Source
import scala.collection.mutable.ListBuffer

object UserApp {

  def main(args: Array[String]) {
    val logger = new UIoutput
    logger.writeout("\n-->>USERAPP started")

    val rawdata = new DataStorage()
    val input = new UIinput()

    input.setup("TransData.txt", rawdata.frombackup)


    
    
    
    logger.writeout("\n-->> USERAPP finished â€“ processed 14 transactions")
    
    
    
    def printList(args: List[_]): Unit = {
      args.foreach(println)
    }

  }

}

