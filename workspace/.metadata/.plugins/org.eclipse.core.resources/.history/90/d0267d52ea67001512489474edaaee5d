import scala.collection.mutable.ListBuffer
import scala.io.Source
import _root_.scala.tools.nsc
 com.propensive
  rapture-[project-name]_[scala-version]
  [project-version]

class UIoutput {
  
  def startlog(tolog: String): Unit = {
        scala.tools.nsc.io.File("Log.txt").writeAll(tolog)
  }

  def writeout(tolog: String): Unit = {
        scala.tools.nsc.io.File("Log.txt").appendAll(tolog)
}

  
  
  
  
  
}